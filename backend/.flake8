[flake8]
max-line-length = 88
extend-ignore = 
    E203,
    W503,
    E501,
    W293,
    F401,
    F841,
    F402,
    F811,
    F541,
    D100,
    D101,
    D102,
    D103,
    D104,
    D105
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .eggs,
    *.egg,
    alembic
per-file-ignores =
    __init__.py:F401
    tests/*:D
max-complexity = 10
